{
    "name": "Serenergy",
    "category": "TH-E Box",
    "group": "Fuel cell",
    "description": "Serenergy Methanol fuel cell system",
    
    "devices": [
        {
            "name": "Serenergy",
            "driver": "snmp",
            "settings": "SNMPVersion=V2c:AUTHENTICATIONPASSPHRASE=private"
        }
    ],
    
    "options": [
        {
            "id": "ip",
            "name": "IP address",
            "description": "The IP address of the SNMP agent device.",
            "type": "text",
            "mandatory": true,
            "syntax": "deviceAddress"
        },
        {
            "id": "port",
            "name": "Port",
            "description": "The address port of the target SNMP agent device.<br>By convention, SNMP agents use the UDP-Port 161, while sending traps to 162.",
            "type": "text",
            "default": "161",
            "mandatory": true,
            "syntax": "deviceAddress"
        }
    ],
    
    "items": [
        {
            "id": "enable",
            "type": "Switch",
            "label": "Enable",
            "mapping": {
                "ON": { "channel": "sngyh3control_enable", "value": 1 },
                "OFF": { "channel": "sngyh3control_enable", "value": 0 }
            },
            "input": "sngyh3control_enable"
        },
        {
            "id": "module",
            "type": "Switch",
            "label": "Module",
            "mapping": {
                "ON": { "channel": "sngyh3control_start", "value": 3 },
                "OFF": { "channel": "sngyh3control_stop", "value": 3 }
            },
            "input": "sngyh3control_requested_state"
        },
        {
            "id": "power",
            "type": "Slider",
            "label": "Power",
            "format": "%.1f kW",
            "max": 72,
            "min": 24,
            "step": 1,
            "scale": 0.0625,
            "mapping": {
                "SET": { "channel": "sngyh3control_current_limit" }
            },
            "input": "sngyh3control_current_limit"
        },
        {
            "id": "state",
            "type": "Text",
            "label": "State",
            "select": {
                "1":"Passive",
                "2":"Standby",
                "3":"Preheat",
                "4":"Starting",
                "5":"Limited operation",
                "6":"Operation",
                "7":"Shutdown",
                "8":"Emergency shutdown",
                "9":"Error",
                "10":"Zero load mode"
            },
            "input": "sngyh3status_state"
        }
    ],

    "channels": [
        {
            "name": "sngyh3status_state",
            "description": "fc_state",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.3.1.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000},
            "configs": {"samplingInterval": 5000, "valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_enable",
            "description": "fc_control_enable",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.1.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000},
            "configs": {"samplingInterval": 5000, "valueOffset":-1, "valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_requested_state",
            "description": "fc_control_requested_state",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.2.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000},
            "configs": {"samplingInterval": 5000, "valueOffset":-1, "valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_start",
            "description": "fc_control_start",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.3.0",
            "logging": "authorization:NONE",
            "configs": {"valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_stop",
            "description": "fc_control_stop",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.4.0",
            "logging": "authorization:NONE",
            "configs": {"valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_reset",
            "description": "fc_control_reset",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.5.0",
            "logging": "authorization:NONE",
            "configs": {"valueType":"INTEGER"}
        },
        {
            "name": "sngyh3control_current_limit",
            "description": "fc_stack_current_limit",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.2.6.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000},
            "configs": {"samplingInterval": 5000, "scalingFactor":0.1, "valueType":"INTEGER"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_current_limit"}
                }
            ]
        },
        {
            "name": "sngyh3status_stack_power",
            "description": "fc_stack_power",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.3.9.1.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000, "loggingTolerance": 10, "average": true},
            "configs": {"samplingInterval": 5000, "valueType":"DOUBLE"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_power"}
                },
                {
                    "process": "power_to_kwh",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_energy"}
                }
            ]
        },
        {
            "name": "sngyh3status_stack_voltage",
            "description": "fc_stack_voltage",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.3.9.2.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000, "loggingTolerance": 0.1, "average": true},
            "configs": {"samplingInterval": 5000, "scalingFactor":0.1, "valueType":"DOUBLE"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_voltage"}
                }
            ]
        },
        {
            "name": "sngyh3status_stack_current",
            "description": "fc_stack_current",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.3.9.3.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000, "loggingTolerance": 0.1, "average": true},
            "configs": {"samplingInterval": 5000, "scalingFactor":0.1, "valueType":"DOUBLE"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_current"}
                }
            ]
        },
        {
            "name": "sngyh3status_stack_temperature",
            "description": "fc_stack_temperature",
            "device": "Serenergy",
            "address": "1.3.6.1.4.1.46029.1.1.1.3.9.4.0",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000, "loggingTolerance": 0.1, "average": true},
            "configs": {"samplingInterval": 5000, "scalingFactor":0.1, "valueType":"DOUBLE"},
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": {"type": "ProcessArg::FEEDID", "value": "fc_stack_temperature"}
                }
            ]
        }
    ],

    "feeds": [
        {
            "name": "fc_stack_power",
            "tag": "Power",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "fc_stack_energy",
            "tag": "Energy",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "fc_stack_voltage",
            "tag": "Voltage",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "fc_stack_current",
            "tag": "Current",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "fc_stack_current_limit",
            "tag": "Current",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        },
        {
            "name": "fc_stack_temperature",
            "tag": "Temperature",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPTIMESERIES"
        }
    ],
    
    "syntax": {
        "deviceAddress": {
            "keyValue": false,
            "separator": "/"
        }
    }
}
