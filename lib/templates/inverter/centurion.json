            "address": "GRID_VOLTAGE",
            "logging": {"loggingInterval": 5000, "loggingMaxInterval": 60000, "loggingTolerance": 0.01, "average": true},
            "configs": {"samplingInterval": 5000, "valueType":"DOUBLE"},
            "processList": [
                {
                    "process": "log_to_feed",
